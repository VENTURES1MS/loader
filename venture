local Players = game:GetService("Players")

local scripts = {
    [16732694052] = "https://raw.githubusercontent.com/VENTURES1MS/loader/refs/heads/main/GamesData/Fisch.lua",
    [87039211657390] = "https://raw.githubusercontent.com/VENTURES1MS/loader/main/scripts/GameB.lua",
}

local placeId = tostring(game.PlaceId)
local scriptUrl = scripts[placeId]

if scriptUrl then
    loadstring(game:HttpGet(scriptUrl))()
else
    Players.LocalPlayer:Kick("Game not supported by this script.")
end
